{
  "name": "rflabs-marketing-website",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "serve": "node dist/server.js",
    "start": "babel-node src/server.js",
    "start:dev": "nodemon src/server.js --exec babel-node",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\""
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  },
  "author": "klee",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@sendgrid/mail": "^6.5.5",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "lodash": "^4.17.15",
    "uuid": "^7.0.2"
  }
}
